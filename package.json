{
  "name": "full-table",
  "author": "mengen.dai",
  "module": "dist/full-table.esm.js",
  "version": "1.0.5",
  "keywords": [
    "table",
    "editable",
    "data-table",
    "full-table",
    "edit-table",
    "data-management"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/full-table.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/full-table.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ant-design/icons": "^5.3.0",
    "@babel/preset-react": "^7.23.3",
    "@size-limit/preset-small-lib": "^11.0.2",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "antd": "^5.13.3",
    "dayjs": "^1.11.10",
    "husky": "^9.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.70.0",
    "size-limit": "^11.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
